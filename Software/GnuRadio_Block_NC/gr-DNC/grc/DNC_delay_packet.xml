<?xml version="1.0"?>
<block>
  <name>Delay Packet</name>
  <key>DNC_delay_packet</key>
  <category>DNC</category>
  <import>import DNC</import>
  <make>DNC.delay_packet($data_size, $max_delay)</make>
  <param>
    <name>Data Size</name>
    <key>data_size</key>
    <type>int</type>
  </param>
  <param>
    <name>Max Delay</name>
    <key>max_delay</key>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
