<?xml version="1.0"?>
<block>
  <name>Check Received Packet</name>
  <key>DNC_check_received_packet</key>
  <category>DNC</category>
  <import>import DNC</import>
  <make>DNC.check_received_packet($data_size, $check_node_id)</make>
  <param>
    <name>Data Size</name>
    <key>data_size</key>
    <type>int</type>
  </param>
  <param>
    <name>Check Node ID</name>
    <key>check_node_id</key>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
