<?xml version="1.0"?>
<block>
  <name>1/2 Convolutional Decoder</name>
  <key>DNC2_convolutional_decoder</key>
  <category>DNC2</category>
  <import>import DNC2</import>
  <make>DNC2.convolutional_decoder($gen1, $gen2)</make>

  <param>
    <name>Gen Metrix Path</name>
    <key>gen1</key>
    <value>[1, 1, 1]</value>
    <type>raw</type>
  </param>

  <param>
    <name>Gen Metrix Path 2</name>
    <key>gen2</key>
    <value>[1, 0, 1]</value>
    
    <type>raw</type>
  </param>

  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>

  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
