<?xml version="1.0"?>
<block>
  <name>Image Text Sink</name>
  <key>hoang_image_text_sink</key>
  <category>hoang</category>
  <import>import hoang</import>
  <make>hoang.image_text_sink($filename)</make>

  <param>
    <name>File</name>
    <key>filename</key>
    <type>file_open</type>
  </param>

  <sink>
    <name>in</name>
    <type>int</type>
    <vlen>1</vlen>
  </sink>
</block>
