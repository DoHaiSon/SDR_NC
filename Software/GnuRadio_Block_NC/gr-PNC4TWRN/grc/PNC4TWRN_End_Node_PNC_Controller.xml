<?xml version="1.0"?>
<block>
  <name>End Node PNC Controller</name>
  <key>PNC4TWRN_End_Node_PNC_Controller</key>
  <category>PNC4TWRN</category>
  <import>import PNC4TWRN</import>
  <make>PNC4TWRN.End_Node_PNC_Controller($fft_length, $occupied_tones, $cp_length, $node)</make>
	<param>
		<name>FFT Length</name>
		<key>fft_length</key>
		<value>64</value>
		<type>int</type>
	</param>
	<param>
		<name>Occupied Tones</name>
		<key>occupied_tones</key>
		<value>48</value>
		<type>int</type>
	</param>
	<param>
		<name>CP Length</name>
		<key>cp_length</key>
		<value>16</value>
		<type>int</type>
	</param>
	  <param>
	<name>Node</name>
	<key>node</key>
	<value>0</value>
	<type>enum</type>
	<option>
		<name>A</name>
		<key>0</key>
	</option>
	<option>
		<name>B</name>
		<key>1</key>
	</option>
	</param>
	<sink>
		<name>in0</name>
		<type>complex</type>
	</sink>
	<sink>
		<name>in1</name>
		<type>byte</type>
	</sink>
	<source>
		<name>out0</name>
		<type>complex</type>
	</source>
	<source>
		<name>out1</name>
		<type>byte</type>
	</source>
</block>
